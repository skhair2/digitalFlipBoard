{
  "name": "digital-flipboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "vite",
    "dev:monorepo": "turbo run dev --parallel",
    "build": "vite build",
    "build:monorepo": "turbo run build",
    "lint": "eslint .",
    "type-check": "tsc -b",
    "preview": "vite preview",
    "server": "cd packages/api && npm run start",
    "server:dev": "cd packages/api && npm run dev",
    "server:install": "npm install"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.9",
    "@heroicons/react": "^2.1.1",
    "@supabase/supabase-js": "^2.39.3",
    "clsx": "^2.1.0",
    "date-fns": "^3.3.1",
    "dompurify": "^3.0.8",
    "framer-motion": "^11.0.3",
    "gsap": "^3.13.0",
    "mixpanel-browser": "^2.49.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^2.0.4",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.22.0",
    "resend": "^6.5.2",
    "socket.io-client": "^4.7.4",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "terser": "^5.27.0",
    "turbo": "^1.10.16",
    "typescript": "^5.3.3",
    "vite": "^5.1.0"
  }
}
